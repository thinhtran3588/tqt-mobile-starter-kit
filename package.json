  "name": "mobile",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-alpha": "cd android && fastlane android alpha && cd ..",
    "android-build": "cd android && fastlane android build && cd ..",
    "android-build-apk": "cd android && fastlane android build_apk && cd ..",
    "android-clean": "cd android && fastlane android clean && cd ..",
    "android-hashkey": "keytool -exportcert -alias mobile -keystore android/app/mobile.keystore | openssl sha1 -binary | openssl base64",
    "android-release": "react-native run-android --variant=release",
    "android-signing": "cd android && ./gradlew signingReport && cd ..",
    "codepush-android": "source '.env' && appcenter codepush release-react --sourcemap-output --app $CODE_PUSH_ANDROID_APP_ID --output-dir ./build --description \"${GITHUB_WORKFLOW}-${GITHUB_RUN_NUMBER}\"",
    "codepush-android-sentry": "source '.env' && export SENTRY_PROPERTIES=android/sentry.properties && sentry-cli react-native appcenter $CODE_PUSH_ANDROID_APP_ID android ./build/CodePush --dist \"${GITHUB_WORKFLOW}-${GITHUB_RUN_NUMBER}\"",
    "codepush-android-production": "source '.env' && appcenter codepush promote -a $CODE_PUSH_ANDROID_APP_ID -s Staging -d Production",
    "codepush-history": "source '.env' && echo Android && echo Staging && appcenter codepush deployment history Staging -a $CODE_PUSH_ANDROID_APP_ID && echo Production && appcenter codepush deployment history Production -a $CODE_PUSH_ANDROID_APP_ID && echo iOS && echo Staging && appcenter codepush deployment history Staging -a $CODE_PUSH_IOS_APP_ID && echo Production && appcenter codepush deployment history Production -a $CODE_PUSH_IOS_APP_ID",
    "codepush-ios": "source '.env' && appcenter codepush release-react --sourcemap-output --app $CODE_PUSH_IOS_APP_ID --output-dir ./build --description \"${GITHUB_WORKFLOW}-${GITHUB_RUN_NUMBER}\"",
    "codepush-ios-sentry": "source '.env' && export SENTRY_PROPERTIES=ios/sentry.properties && sentry-cli react-native appcenter $CODE_PUSH_IOS_APP_ID ios ./build/CodePush --dist \"${GITHUB_WORKFLOW}-${GITHUB_RUN_NUMBER}\"",
    "codepush-ios-production": "source '.env' && appcenter codepush promote -a CODE_PUSH_IOS_APP_ID -s Staging -d Production",
    "codepush-login": "source '.env' && appcenter login --token $CODEPUSH_TOKEN",
    "env": "ts-node job.ts env",
    "ios": "react-native run-ios",
    "ios-build": "source '.env' && cd ios && fastlane ios build && cd ..",
    "ios-release": "react-native run-ios --variant=release",
    "ios-testflight": "source '.env' && cd ios && fastlane ios beta && cd ..",
    "lint": "prettier --check . && eslint . --ext .js,.jsx,.ts,.tsx && tsc --noEmit",
    "lint-fix": "prettier --write . --ext .js,.jsx,.ts,.tsx && eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "patches": "sh ./patches/patches.sh",
    "postinstall": "yarn patches && npx jetify && npx pod-install",
    "start": "react-native start",
    "test": "jest  --detectOpenHandles",
    "test-coverage": "jest --coverage  --detectOpenHandles",
    "update-build": "ts-node job.ts update-build",
    "update-icons": "./generate-icons.sh",
    "update-ver": "ts-node job.ts update-ver",
    "validate": "yarn lint && yarn test-coverage"
  },
  "version": "0.10.0"
}
